{
  "version": 2,
  "builds": [
    { "src": "server.ts", "use": "@vercel/node" },
    { "src": "api/**/*.[jt]s", "use": "vercel-deno@3.1.0" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "server.ts" }
  ],
  "buildCommand": "echo 'Skipping TypeScript build'",
  "installCommand": "npm install --omit=dev"
}